@model IEnumerable<Web.Models.ExamResultDto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResultID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject.SubjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTermPoint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MidTermPoint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTermPoint)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ResultID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Subject.SubjectName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Student.StudentName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.StartTermPoint)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MidTermPoint)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EndTermPoint)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.ResultID }) |
        @Html.ActionLink("Details", "Details", new { id = item.ResultID }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.ResultID })
    </td>
</tr>
}

</table>
